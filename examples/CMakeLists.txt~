# build ospTutorial, for demonstration and API testing

# C++ version
add_executable(ospTutorial_mtvCpp ${OSPRAY_RESOURCE} multivariant.cpp)
target_link_libraries(ospTutorial_mtvCpp PRIVATE ospray_sdk)
ospray_sign_target(ospTutorial_mtvCpp)

